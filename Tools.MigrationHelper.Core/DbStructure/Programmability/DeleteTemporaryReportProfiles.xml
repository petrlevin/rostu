    <Name>DeleteTemporaryReportProfiles</Name>
    <CreateCommand>-- =============================================
-- Author:		a.shmelev
-- Create date: 10.05.2013
-- Description:	Удаление временных экземпляров всех сущностей типа "Отчет"
-- =============================================
CREATE PROCEDURE dbo.DeleteTemporaryReportProfiles 
AS
BEGIN
	DECLARE 
		 @entityName NVARCHAR(100)
		,@tmp NVARCHAR(1000);

	DECLARE reportEntities CURSOR FOR 
		SELECT name FROM ref.Entity WHERE idEntityType = 9;

	OPEN reportEntities;

	FETCH NEXT FROM reportEntities INTO @entityName;
	WHILE @@FETCH_STATUS = 0
	BEGIN
		SET @tmp = 'delete from rep.' + @entityName + ' where isTemporary = 1'
		EXEC sp_executesql @tmp;
		FETCH NEXT FROM reportEntities INTO @entityName;
	END
	CLOSE reportEntities;
	DEALLOCATE reportEntities;
END</CreateCommand>
    <idProgrammabilityType>1</idProgrammabilityType>
    <Order></Order>
    <idProject>100</idProject>
    <isDisabled>false</isDisabled>
