CREATE FUNCTION [dbo].[GetSBPParentGRBS](@idSBP [int])
RETURNS [int] 
AS 
BEGIN
	declare @sbpTypeGRBS int,
			@result int;

	select @sbpTypeGRBS = id from enm.SBPType
	where Name = 'GeneralManager';



	with sbpTree as 
	(
		select S1.id, S1.idParent, S1.idSBPType 
			from ref.SBP S1 
			where S1.id = @idSBP
		union all
			select S2.id, S2.idParent, S2.idSBPType
			from ref.SBP S2
			inner join sbpTree T on S2.id = T.idParent
	)
	select @result = T.id
	from sbpTree T
	where T.idSBPType = @sbpTypeGRBS
	
	RETURN @result
END</CreateCommand>
