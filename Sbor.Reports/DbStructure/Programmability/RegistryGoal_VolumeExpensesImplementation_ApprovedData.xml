  <ref.Programmability>
    <id>-1275068365</id>
    <Schema>sbor</Schema>
    <Name>RegistryGoal_VolumeExpensesImplementation_ApprovedData</Name>
    <CreateCommand>CREATE PROCEDURE [sbor].[RegistryGoal_VolumeExpensesImplementation_ApprovedData]( @YearBudget int, @idVersion int, @PPO int, @IdBudget int, @datestate nvarchar(64), @idValueType int, @idValueType1 int)
AS
BEGIN
		SELECT	sge.id AS Id
		,sg.Caption As CaptionGoal
		,VolumeOfExpenses = (SELECT [sbor].[VolumeExpensesImplementation_ApprovedData] (
		   sge.id 
		  ,@YearBudget
		  ,@idVersion
		  ,@PPO
		  ,@IdBudget
		  ,@datestate
		  ,@idValueType
		  ,@idValueType1))
		,VolumeOfExpenses1 = (SELECT [sbor].[VolumeExpensesImplementation_ApprovedData] (
		   sge.id 
		  ,@YearBudget + 1
		  ,@idVersion
		  ,@PPO
		  ,@IdBudget
		  ,@datestate
		  ,@idValueType
		  ,@idValueType1))
		,VolumeOfExpenses2 = (SELECT [sbor].[VolumeExpensesImplementation_ApprovedData] (
		   sge.id 
		  ,@YearBudget + 2
		  ,@idVersion
		  ,@PPO
		  ,@IdBudget
		  ,@datestate
		  ,@idValueType
		  ,@idValueType1))
		from reg.SystemGoalElement AS sge
		INNER JOIN ref.SystemGoal AS sg
		ON sge.idSystemGoal = sg.id
END</CreateCommand>
    <idProgrammabilityType>1</idProgrammabilityType>
