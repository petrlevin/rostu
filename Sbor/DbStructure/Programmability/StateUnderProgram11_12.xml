  <ref.Programmability>
    <id>-1275068391</id>
    <Schema>sbor</Schema>
    <Name>StateUnderProgram11_12</Name>
    <CreateCommand>CREATE FUNCTION [sbor].[StateUnderProgram11_12] (@id int)
RETURNS TABLE
AS
RETURN
(
	--построенна на основе сноске11_12 ПФ ПОДПРОГРАММА ГОСУДАРСТВЕННОЙ ПРОГРАММЫ
SELECT 
		 FS.Caption AS NumberName --источник финансирования
		,SPRMV.Value AS GIValue --данные за годам
		,YEAR(HP.DateEnd) AS NumYear -- год
FROM	
		doc.StateProgram AS SP
		INNER JOIN tp.StateProgram_ResourceMaintenance AS SPRM
			ON SP.id = SPRM.idOwner 
		INNER JOIN ref.FinanceSource AS FS
			ON FS.id = SPRM.idFinanceSource
		LEFT JOIN tp.StateProgram_ResourceMaintenance_Value AS SPRMV
			ON SPRMV.idMaster = SPRM.id
		LEFT JOIN ref.HierarchyPeriod AS HP 
			ON HP.id = SPRMV.idHierarchyPeriod
WHERE
		SP.id = @id
);</CreateCommand>
    <idProgrammabilityType>2</idProgrammabilityType>
    <Order></Order>
    <idProject>500</idProject>
    <isDisabled>false</isDisabled>
  </ref.Programmability>
