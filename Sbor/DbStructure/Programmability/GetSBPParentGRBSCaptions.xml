    <CreateCommand>CREATE FUNCTION [ref].[GetSBPParentGRBSCaptions]()
RETURNS 
@result TABLE 
(
	id int PRIMARY KEY,
	GRBS_Caption nvarchar(400)
)
AS
BEGIN
	with cte as ( 
	select id as idRoot, id, Caption from ref.SBP where idSBPType=1
	union all
	SELECT a.idRoot, b.id, a.Caption from cte a
		inner join ref.SBP b on b.idParent=a.id
	)
	INSERT INTO @result
	select a.id, a.Caption from cte a
	RETURN 
END
</CreateCommand>
