    <CreateCommand>CREATE FUNCTION doc.LimitBudgetAllocations_GetYearSums()
RETURNS 
@result TABLE 
(
	id int,
	OFG numeric(22,2),
	PFG1 numeric(22,2),
	PFG2 numeric(22,2)
)
AS
BEGIN
	INSERT INTO @result
	SELECT idOwner, SUM(OFG) as OFG, SUM(PFG1) as PFG1, SUM(PFG2) as PFG2
	FROM [tp].[LimitBudgetAllocations_LimitAllocations]
	GROUP BY idOwner
