  <ref.Programmability>
    <id>-2080374743</id>
    <Schema>doc</Schema>
    <Name>PublicInstitutionEstimate_GetYearExpenses</Name>
    <CreateCommand>CREATE FUNCTION doc.PublicInstitutionEstimate_GetYearExpenses()
RETURNS 
@result TABLE 
(
	id INT,
	OFG NUMERIC(22,2),
	PFG1 NUMERIC(22,2),
	PFG2 NUMERIC(22,2)
)
AS
BEGIN
	INSERT INTO @result 
	SELECT id, SUM(OFG) as OFG,
		SUM(PFG1) as PFG1,
		SUM(PFG2) as PFG2 
	FROM (	SELECT idOwner as id, OFG, PFG1, PFG2
			FROM [tp].[PublicInstitutionEstimate_Expense] 
			UNION ALL 
			SELECT idOwner as id, OFG, PFG1, PFG2 
			FROM [tp].[PublicInstitutionEstimate_FounderAUBUExpense]) a
	GROUP BY id
	RETURN 
END</CreateCommand>
